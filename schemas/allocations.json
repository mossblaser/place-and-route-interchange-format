{
    "$schema": "http://json-schema.org/draft-04/schema#",
    
    "title": "For each vertex, gives the chip resources it was allocated on the chip it is placed on.",
    "type": "object",
    "additionalProperties": {"$ref": "#/resources/resourceAllocation"},
    
    "resources": {
        "resourceRange": {
            "description": "A range a of resource to be consumed.",
            "type": "array",
            "items": [
                {"description": "Start of range (inclusive)", "type": "integer"},
                {"description": "End of range (exclusive)", "type": "integer"}
            ]
        },
        "resourceAllocation": {
            "description": "Enumerates allocated on-chip resources.",
            "type": "object",
            "additionalProperties": {"$ref": "#/resources/resourceRange"}
        }
    }
}
